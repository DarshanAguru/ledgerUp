{
  "expo": {
    "name": "ledgerup",
    "slug": "ledgerup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appstore.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.darshanaguru.ledgerup"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appstore.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/appstore.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "<-- PROJECT ID -->"
      }
    }
  }
}
